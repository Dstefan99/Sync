<Window x:Class="Sync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sync"
        mc:Ignorable="d"
        Title="MainWindow" Height="676.569" Width="987.762" Loaded="Window_Loaded">
    <Grid Background="LightGray">
        <WebBrowser x:Name="Sync_Browser" Margin="-2,53,2,38"/>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="38" VerticalAlignment="Top" Width="980" Margin="0,608,0,0">
            <TabItem Header="TabItem" Margin="-2,-2,-43,0">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <TextBox x:Name="textBox" Height="23" Margin="0,30,75,0" TextWrapping="Wrap" VerticalAlignment="Top" BorderThickness="2"/>
        <Button x:Name="button" Content="Search" Margin="905,30,0,0" VerticalAlignment="Top" Height="23" Click="button_Click"/>

    </Grid>
</Window>
